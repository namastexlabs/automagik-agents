    },
    "cor": {
      "type": "string",
      "description": "Cor do cabo",
      "enum": [
        "preto",
        "branco",
        "cinza",
        "vermelho",
        "azul",
        "verde"
      ]
    },
    "blindagem": {
      "type": "string",
      "description": "Tipo de blindagem do cabo",
      "enum": [
        "nenhuma",
        "folha",
        "trançada",
        "dupla"
      ]
    },
    "garantia": {
      "type": "integer",
      "description": "Tempo de garantia do produto em meses",
      "minimum": 0
    },
    "peso": {
      "type": "number",
      "description": "Peso do cabo em gramas",
      "minimum": 1
    }
  }
}