{
  "family_id": 96,
  "family_name": "CABOS",
  "specs_schema": {
    "comprimento": {
      "type": "number",
      "description": "Comprimento do cabo em metros",
      "minimum": 0.1
    },
    "tipo_conector": {
      "type": "string",
      "description": "Tipo de conector nas extremidades do cabo",
      "enum": [
        "USB-A",
        "USB-C",
        "HDMI",
        "VGA",
        "RJ45",
        "DisplayPort",
        "Jack 3.5mm"
      ]
    },
    "revestimento": {
      "type": "string",
      "description": "Material de revestimento do cabo",
      "enum": [
        "PVC",
        "Nylon",
        "TPE"
      ]
    },
    "suporte_audio": {
      "type": "boolean",
      "description": "Indica se o cabo suporta transmissão de áudio"
    },
    "suporte_video": {
      "type": "boolean",
      "description": "Indica se o cabo suporta transmissão de vídeo"
    },
    "taxa_transmissao_dados": {
      "type": "number",
      "description": "Taxa máxima de transmissão de dados em Gbps",
