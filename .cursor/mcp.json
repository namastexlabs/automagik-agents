{
    "mcpServers": {
      "postgres_automagik_agents": {
        "command": "npx",
        "args": [
          "-y",
          "@modelcontextprotocol/server-postgres",
          "postgresql://postgres:postgres@localhost:35432/automagik_agents"
        ]
      },
      "deepwiki": {
        "command": "npx",
        "args": [
          "-y",
          "supergateway",
