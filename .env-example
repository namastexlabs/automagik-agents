# Authentication
AUTOMAGIK_AGENTS_API_KEY=your_api_key_here  # Required for accessing protected endpoints

# Server Configuration
AUTOMAGIK_AGENTS_PORT=8000
AUTOMAGIK_AGENTS_HOST=0.0.0.0
AUTOMAGIK_AGENTS_ENV=development  # development, production, testing

# Logging Configuration
AUTOMAGIK_AGENTS_LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL
AUTOMAGIK_AGENTS_LOGFIRE_TOKEN=your_logfire_token  # Optional, for logging service
AUTOMAGIK_AGENTS_LOGFIRE_IGNORE_NO_CONFIG=true  # Optional, suppress Logfire warning

# OpenAI API Configuration
AUTOMAGIK_AGENTS_OPENAI_API_KEY=your_openai_api_key

# Notion Integration (Optional)
AUTOMAGIK_AGENTS_NOTION_TOKEN=your_notion_integration_token
